google_cloud_bucket_name(ENV["GOOGLE_CLOUD_BUCKET_NAME"])

storage_mode("google_cloud")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.baptistelecat.weatherjourney"]) # The bundle identifier of your app
username(ENV["APPLE_DEVELOPER_USERNAME"]) # Your Apple Developer Portal username

